package executable;

import java.util.Collection;

import edu.uci.ics.jung.graph.DirectedSparseMultigraph;

import graphElements.Edge;
import graphElements.Node;
import utilities.GraphReaderJUNG;
import utilities.GraphReader;

public class CommunityViz {

	DirectedSparseMultigraph<Node, Edge> graph;
	GraphReader reader;

	public CommunityViz() {
		reader = new GraphReader("./data/graphs/Risk.graphml");
		graph = reader.getGraph();
		Collection<Edge> edges = graph.getEdges();
		for(Edge e: edges){
			System.out.println(e.getSource());
			e.getSource().getName();
			e.getTarget().getName();
		}
	}

	public static void main(String[] args) {
		CommunityViz graph = new CommunityViz();
		System.out.println("Done");
	}
}
