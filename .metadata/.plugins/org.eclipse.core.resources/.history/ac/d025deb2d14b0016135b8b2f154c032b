package executable;

import org.gicentre.utils.move.*;

import gui.Zoom;
import processing.core.*;

public class Executable extends PApplet {
	Logica app;
	boolean clockwise = true;
	PVector mousePos;
	Zoom z;

	public void setup() {
		size(1200, 600);
		// size(1500, 600);
	//	app = new Logica(this);
		textSize(10);
		textAlign(CENTER);
		// noLoop();
		ellipseMode(CENTER);
		z = new Zoom(this);
	}

	public void draw() {
		background(0);
		//app.show(this);
		
		ellipse(10,10,10,10);
	}

	public void keyPressed() {
		if (key == 'r') {
			clockwise = !clockwise;
		}
	}

}
