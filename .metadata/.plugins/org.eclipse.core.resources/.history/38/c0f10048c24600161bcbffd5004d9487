package utilities.geometry;

import java.awt.geom.Point2D;

import org.apache.commons.collections15.Transformer;
import processing.core.*;
import graphElements.Node;

public class NodeToPoint2D implements Transformer<Node, Point2D> {

	public NodeToPoint2D() {

	}

	@Override
	public Point2D transform(Node arg) {
		Point2D point = new Point2D.Float(arg.getX()+100, arg.getY()+300);
		return point;
	}

}
